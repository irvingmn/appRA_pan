            android:defaultValue="true" />
            
        <CheckBoxPreference android:key="@string/key_title_on"
            android:title="@string/title_title_on" android:summary="@string/summary_title_on"
            android:defaultValue="true" />
            
        <CheckBoxPreference android:key="@string/key_key_on"
            android:title="@string/title_key_on" android:summary="@string/summary_key_on"
            android:defaultValue="true" />
            
        <CheckBoxPreference android:key="@string/key_range_label_on"
